    "react-native-udp": "^2.3.1",
    "react-native-vector-icons": "^4.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-native": "^4.2.0",
    "readable-stream": "^1.0.33",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rn-nodeify": "^10.0.0",
    "stream-browserify": "^1.0.0",
    "styled-components": "^3.2.3",
    "timers-browserify": "^1.4.2",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
