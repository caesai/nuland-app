{
  "name": "NuLand",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest-expo": "25.0.0",
    "metro": "^0.31.0",
    "metro-core": "^0.31.0"
  },
  "main": "./index.js",
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js",
    "checkDep": "cd android/ && ./gradlew -q dependencies app:dependencies --configuration compile",
    "postinstall": "./node_modules/.bin/rn-nodeify --install --hack",
    "clean": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "restart": "echo 999999 | sudo tee -a /proc/sys/fs/inotify/max_user_watches && echo 999999 | sudo tee -a /proc/sys/fs/inotify/max_queued_events && echo 999999 | sudo tee -a /proc/sys/fs/inotify/max_user_instances && watchman shutdown-server && sudo sysctl -p",
    "start": "react-native start",
    "start-dev": "npm install && react-native start",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android": "react-native run-android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "rnpm": {
    "assets": [
      "./src/wasm/"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@mapbox/react-native-mapbox-gl": "^6.1.2-beta",
