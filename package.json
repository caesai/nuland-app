    "fs": "0.0.1-security",
    "https-browserify": "0.0.1",
    "lodash": "^4.17.10",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "16.2.0",
    "react-map-gl": "^3.2.7",
    "react-native": "0.53.3",
    "react-native-bip39": "^2.3.0",
    "react-native-camera": "^1.0.2",
